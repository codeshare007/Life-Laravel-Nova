{
    "name": "christophrumpel/nova-notifications",
    "description": "A Laravel Nova tool for handling Laravel Notifications.",
    "keywords": [
        "laravel",
        "nova",
        "notifications"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "~1.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Christophrumpel\\NovaNotifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Christophrumpel\\NovaNotifications\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Christophrumpel\\NovaNotifications\\ToolServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
