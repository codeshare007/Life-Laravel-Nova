{
  "name": "wqa/nova-sortable-table-resource",
  "description": "A nova sortable table resource",
  "license": "MIT",
  "keywords": [
    "nova",
    "sortable",
    "table",
    "resource"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Adam Lee",
      "email": "adam.lee@wearewqa.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "~5.7.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.7.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Wqa\\NovaSortableTableResource\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wqa\\NovaSortableTableResource\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wqa\\NovaSortableTableResource\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
