{
  "name": "wqa/pdf-extract",
  "description": "A pdf extract",
  "license": "MIT",
  "keywords": [
    "pdf",
    "extract"
  ],
  "type": "library",
  "require": {
    "php": ">=7.0",
    "illuminate/support": "~5.7.0",
    "smalot/pdfparser": "^0.14.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.7.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Wqa\\PdfExtract\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wqa\\PdfExtract\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wqa\\PdfExtract\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
